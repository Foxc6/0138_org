<% flash.each do |key, value| %>
<div id="error_explanation" class="row notice"><%= value %></div>
<% end %>
<div class="row bar-header full-bg">
  <div class="col-sm-12 col-md-12">
    <div class="has-limit">
      <h2>Dashboard</h2>
    </div>
  </div>
</div>




<% if @customer_regimens %>
<div class="group-contents">

  <div>
    <h2>Customer Regimens</h2>
      <ul>
        <li><%= @dashboard_data[:total_regimens] %></li>
      </ul>
  </div>

  <div>
    <h2>Benefits</h2>
    <%= pie_chart(@dashboard_data[:benefits], library:{sliceVisibilityThreshold:0} ) %>
  </div>

  <div>
    <h2>Age Ranges</h2>
    <%= pie_chart(@dashboard_data[:age_ranges], library:{sliceVisibilityThreshold:0}) %>
  </div>

  <div>
    <h2>Skin Types</h2>
    <%= pie_chart(@dashboard_data[:skin_types], library:{sliceVisibilityThreshold:0}) %>
  </div>

  <div>
    <h2>Guides</h2>
    <%= column_chart(@dashboard_data[:guides], library:{sliceVisibilityThreshold:0}) %>
  </div>
</div>
<% end %>
